{
  "expo": {
    "name": "Bullet Journal",
    "slug": "bulletjournal",
    "owner": "oleksandrshysh",
    "originalFullName": "@frankmelward/bulletjournal",
    "version": "1.3.8",
    "orientation": "portrait",
    "icon": "./assets/images/mainIcon.png",
    "scheme": "com.frankmelward.bulletjournal",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/adaptive-icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#19677f"
    },
    "androidStatusBar": {
      "barStyle": "light-content"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.frankmelward.bulletjournal"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundImage": "./assets/images/iconBg.png",
        "backgroundColor": "#354352"
      },
      "versionCode": 39,
      "googleServicesFile": "./google-services.json",
      "package": "com.frankmelward.bulletjournal"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      "@react-native-firebase/crashlytics",
      "expo-image-picker",
      "./plugins/custom-android-styles.js",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "6359b66b-9eb1-4131-88a0-e25cc5f603f9"
      }
    }
  }
}
