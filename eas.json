{
  "cli": {
    "version": ">= 3.16.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "env": {
        "EXPO_PUBLIC_API_URL": "EXPO_PUBLIC_API_URL",
        "EXPO_PUBLIC_FIREBASE_API_KEY": "EXPO_PUBLIC_FIREBASE_API_KEY",
        "EXPO_PUBLIC_ANDROID_CLIENT_ID": "EXPO_PUBLIC_ANDROID_CLIENT_ID",
        "EXPO_PUBLIC_BUCKET": "EXPO_PUBLIC_BUCKET",
        "EXPO_PUBLIC_AWS_ACCESS_KEY_ID": "EXPO_PUBLIC_AWS_ACCESS_KEY_ID",
        "EXPO_PUBLIC_AWS_SECRET_ACCESS_KEY": "EXPO_PUBLIC_AWS_SECRET_ACCESS_KEY",
        "EXPO_PUBLIC_AWS_IMAGE_URL": "EXPO_PUBLIC_AWS_IMAGE_URL"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "production"
      }
    }
  }
}
